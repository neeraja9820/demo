def sort(nums):      #using sort the values
    for i in range(len(nums)-1,0,-1):   #using aasending order to decending ordef
        for j in range (i):
            if nums[j]>nums[j+1]:  #swap the values
                temp=nums[j]
                nums[j]=nums[j+1]
                nums[j+1]=temp


nums=[5,4,7,2,9]
sort(nums)

print(nums)